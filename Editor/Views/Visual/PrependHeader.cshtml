<div id="autogeneratedheader">
    @{
        string hdRootpath = "http://localhost:2525/";
        string hdActionName = this.ViewContext.RouteData.Values["action"].ToString();
        string hdControllerName = this.ViewContext.RouteData.Values["controller"].ToString();
        string hdAreaName = "";
        if (this.ViewContext.RouteData.DataTokens["area"] != null)
        {
            hdAreaName = this.ViewContext.RouteData.DataTokens["area"].ToString();
        }
        string hdScriptpath = hdRootpath + "Scripts";
        string hdStylepath = hdRootpath + "Content";
    }
</div>
<style>
    #generatediframe {
    width: 100%;
    overflow: hidden;
    /* zoom: 81%; */
    height: 200px;
    border: 0px;
    }
</style>
<script>
    var hd_rooturl="@Url.Action("", "", new { area = "" })";
    var hd_contenturl="@hdRootpath"+"Content/";
    var hd_function="@hdActionName";
    var hd_controller="@hdControllerName";
    var hd_area = "@hdAreaName";
    var hd_scripturl = "@hdScriptpath";
    var hd_styleurl = "@hdStylepath";
</script>
<script src="@hdScriptpath/internal/htmldesignerinitialize.js" type="text/javascript"></script>
