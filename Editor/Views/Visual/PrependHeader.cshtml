<div id="autogeneratedheader">
    @{
        string actionName = this.ViewContext.RouteData.Values["action"].ToString();
        string controllerName = this.ViewContext.RouteData.Values["controller"].ToString();
        string areaName = "";
        if (this.ViewContext.RouteData.DataTokens["area"] != null)
        {
            areaName = this.ViewContext.RouteData.DataTokens["area"].ToString();
        }
    }
</div>
<style>
    #generatediframe {
    width: 100%;
    overflow: hidden;
    /* zoom: 81%; */
    height: 200px;
    border: 0px;
    }
</style>
<script>
    var hd_rooturl="@Url.Action("", "", new { area = "" })";
    var hd_contenturl="@Url.Content("~/Content/")";
    var hd_function="@actionName";
    var hd_controller="@controllerName";
    var hd_area="@areaName";
</script>
<iframe id="generatediframe" src="generatedurl"></iframe>