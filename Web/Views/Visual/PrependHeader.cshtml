<div id="autogeneratedheader">
    @{
        string cdRootpath = "http://localhost:2525/";
        string hdActionName = this.ViewContext.RouteData.Values["action"].ToString();
        string hdControllerName = this.ViewContext.RouteData.Values["controller"].ToString();
        string hdAreaName = "";
        if (this.ViewContext.RouteData.DataTokens["area"] != null)
        {
            hdAreaName = this.ViewContext.RouteData.DataTokens["area"].ToString();
        }
        string cdScriptpath = cdRootpath + "Scripts";
        string cdStylepath = cdRootpath + "Content";
    }
</div>
<style>
    #generatediframe {
    width: 100%;
    overflow: hidden;
    /* zoom: 81%; */
    height: 200px;
    border: 0px;
    }
</style>
<script>
    /*Current Project Paths*/
    var hd_rooturl = "@Url.Action("", "", new { area = "" })";
    var hd_function = "@hdActionName";
    var hd_controller = "@hdControllerName";
    var hd_area = "@hdAreaName";
    /*Code editor Paths*/
    var cd_rooturl = "@cdRootpath";
    var cd_contenturl = "@cdRootpath" + "Content/";
    var cd_scripturl = "@cdScriptpath";
    var cd_styleurl = "@cdStylepath";
</script>
<script src="@cdScriptpath/internal/localcache.js" type="text/javascript"></script>
<script src="@cdScriptpath/internal/editorinitialize.js" type="text/javascript"></script>
